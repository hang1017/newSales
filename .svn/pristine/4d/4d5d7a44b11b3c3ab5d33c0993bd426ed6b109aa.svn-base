.helpRuleModalStyle {
  width: 658px;
  font-size: 28px;

  .text {
    font-size: 28px;
    font-weight: 400;
    color: #333333;
    line-height: 40px;
    text-align: left;
  }

  .btnDiv {
    display: flex;
    align-items: center;
    justify-content: center;
    padding: 70px 0 50px 0;

    .btn {
      width: 344px;
      height: 96px;
      background: linear-gradient(179deg, #82C9FF 0%, #006DE3 100%);
      border-radius: 48px;
      font-size: 34px;
      font-weight: 600;
      color: #FFFFFF;
      line-height: 48px;
      display: flex;
      align-items: center;
      justify-content: center;
    }
  }


  :global {
    .am-modal-content {
      border-radius: 24px;
    }
  }
}