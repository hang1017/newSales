.indexListStyle {
  height: 100%;
  overflow-y: auto !important;
  -webkit-overflow-scrolling: touch;

  .indeListContent {
    display: flex;
    flex-direction: column;
    height: 120%;
  }

  .ilTopBg {
    position: relative;
    background-repeat: no-repeat;
    // background-image: url('../../../assets/img/customer/indexListBg.png');
    // height: 500px;
    background-size: 100% 100%;

    .ilTopImg {
      width: 100%;
    }

    .ilTopText {
      position: absolute;
      top: 20px;
      right: 0;
      left: 0;
      color: #000;
      font-weight: 400;
      font-size: 36px;
      line-height: 50px;
      text-align: center;
    }

    .ilTop {
      position: fixed;
      top: 283px;
      right: 0;
      z-index: 3;
      display: flex;
      align-items: center;

      .photo {
        width: 80px;
        height: 80px;
        margin-right: 10px;
        border-radius: 50%;
      }
    }

    .ilTopPerson {
      display: flex;
      align-items: center;
      justify-content: center;
      // width: 150px;
      padding: 6px;
      color: #e07485;
      font-weight: 600;
      font-size: 26px;
      background-color: #2ca5ff;
      background-image: linear-gradient(140deg, rgb(252, 240, 192) 0%, rgb(243, 161, 178) 100%);
      border-top-left-radius: 40px;
      border-bottom-left-radius: 40px;
      box-shadow: -2px 10px 8px 0 rgb(243, 161, 178);

      .ava {
        width: 40px;
        height: 40px;
        margin: 0 6px;
        background-image: linear-gradient(140deg, rgb(252, 240, 192) 0%, rgb(243, 161, 178) 100%);
        border-radius: 50%;
      }
    }
  }

  .ilContent {
    z-index: 2;
    display: flex;
    // flex: 1;
    width: 100%;
    height: 90vh;
    margin-top: -10px;
    overflow: auto;
    background-color: #fff;
    border-radius: 24px;

    .ilLeft {
      width: 25%;
      height: 100%;
    }

    .ilRight {
      width: 75%;
      padding-left: 32px;
      overflow: auto;
    }
  }

  .heights {
    height: 120px;
  }

  .cartFootDiv {
    position: fixed;
    right: 0;
    bottom: 0;
    left: 0;
    z-index: 12;
  }
}