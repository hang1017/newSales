.fileModalstyle {
  height: 70vh;
  padding: 20px;
  background-color: #fff;
  border-radius: 30px 30px 0 0;

  .modalContent {
    display: flex;
    flex-direction: column;
    height: 100%;
  }

  .alertHead {
    display: flex;

    .leftBack {
      img {
        width: 48px;
      }

      div {
        width: 48px;
      }
    }

    .alertTitle {
      flex: 1;
      color: rgba(4, 28, 30, 1);
      font-weight: 500;
      font-size: 36px;
      text-align: center;
    }

    .rightClose {
      text-align: right;

      img {
        width: 48px;
      }
    }
  }

  // :global {
  //   .am-modal-content {
  //     border-radius: 30px 30px 0 0;
  //   }
  // }

  .htmlContent {
    padding: 20px;
    flex: 1;
    overflow: auto;
  }
}