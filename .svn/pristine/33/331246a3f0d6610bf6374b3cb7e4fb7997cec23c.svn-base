.progressView {
  position: relative;
  display: flex;
  flex-direction: row;
  align-items: center;
  padding: 38px 30px 30px;
  background-color: #fff;
  border-radius: 24px;

  .leftIcon {
    align-self: flex-start;
    width: 32px;
    height: 32px;
    margin-top: 7px;
  }

  .rightIcon {
    width: 32px;
    height: 32px;
  }

  .contentModel {
    flex: 1;
    width: 0;
    padding: 0 20px;

    .title {
      color: rgba(6, 4, 30, 1);
      font-size: 28px;
      line-height: 48px;
    }

    .subTitle {
      padding-top: 4px;
      color: rgba(149, 150, 156, 1);
      font-size: 24px;
      line-height: 34px;
    }
  }
}
