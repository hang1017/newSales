.trackPage {
  height: calc(100vh);
  overflow-y: scroll;
  background: #f5f7f9;

  .trackBlock {
    margin-top: 24px;
    padding: 24px;
    background: #fff;
    border-radius: 24px;
  }

  .orderBaseInfo {
    margin-bottom: 24px;
    padding: 90px 24px 28px 24px;
    color: rgba(149, 150, 156, 1);
    font-weight: 400;
    font-size: 28px;
    background: #fff;

    & > div {
      margin-top: 16px;
    }

    span {
      color: rgba(6, 4, 30, 1);
    }
  }

  .orderTracking {
    background: rgba(255, 255, 255, 1);
    border-radius: 24px;
  }
}
