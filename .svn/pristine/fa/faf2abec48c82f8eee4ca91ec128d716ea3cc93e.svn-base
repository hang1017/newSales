.center {
  display: flex;
  flex-direction: column;
  align-items: center;
  padding-top: 168px;

  .img {
    width: 112px;
    height: 112px;
  }

  .successContent {
    margin-top: 48px;
    color: #041c1e;
    font-size: 32px;
  }

  .countView {
    margin-top: 16px;
    color: #696d6c;
    font-size: 24px;

    span {
      color: #f53c54;
    }
  }

  .btn {
    width: 292px;
    margin-top: 80px;
    padding: 16px 0;
    color: #ff483d;
    text-align: center;
    border: 1px solid #ff483d;
    border-radius: 36px;
  }
}
